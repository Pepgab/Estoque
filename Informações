package controller;

public class Instrucoes {

	public String nome;
	public int quantidade;
	public double preco;
	
public Instrucoes(String nome, int quantidade, double preco) {
	this.nome = nome;
	this.quantidade = quantidade;
	this.preco = preco;
}
	
public String toString() {
	return "Item: " + nome + " Quantidade: " + quantidade + " Pre√ßo (R$): " + String.format("%.2f", preco);
}
}
